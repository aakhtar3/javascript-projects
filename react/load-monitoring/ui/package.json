{
  "name": "ui",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "rm -rf dist && webpack  --display-error-details --progress --profile --colors --config ./webpack.config.js",
    "lint": "node_modules/.bin/eslint --fix ./src ./test",
    "start": "webpack-dev-server --hot",
    "test": "jest"
  },
  "dependencies": {
    "connected-react-router": "^6.5.2",
    "d3-format": "^1.3.2",
    "history": "^4.9.0",
    "pondjs": "^0.8.10",
    "prop-types": "^15.7.2",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-timeseries-charts": "^0.16.1",
    "react-toastify": "^5.3.2",
    "redux": "^4.0.4",
    "superagent": "^5.1.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.8.0",
    "react-jest": "^0.2.1",
    "redux-test-utils": "^0.3.0",
    "sinon": "^7.2.4",
    "style-loader": "^1.0.0",
    "superagent-mock": "^3.7.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|git)$": "<rootDir>/test/mock/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/test/mock/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/"
    ],
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "lcov"
    ]
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "browser": true,
      "jest": true
    },
    "globals": {
      "expect": true,
      "window": true,
      "document": true
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "no-trailing-spaces": "error",
      "react/jsx-filename-extension": 0,
      "import/no-named-as-default": 0,
      "no-mixed-operators": 0,
      "react/forbid-prop-types": 0,
      "class-methods-use-this": 0,
      "react/prop-types": 0,
      "import/prefer-default-export": 0,
      "react/require-default-props": 0,
      "no-unused-vars": 0,
      "no-unused-expressions": [
        2,
        {
          "allowShortCircuit": true
        }
      ],
      "comma-dangle": "error",
      "arrow-parens": "error",
      "eol-last": [
        "error",
        "always"
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "backtick"
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-spacing": [
        1,
        {
          "before": false,
          "after": true
        }
      ],
      "template-curly-spacing": [
        "error",
        "always"
      ],
      "keyword-spacing": "error",
      "space-infix-ops": "error"
    }
  }
}
